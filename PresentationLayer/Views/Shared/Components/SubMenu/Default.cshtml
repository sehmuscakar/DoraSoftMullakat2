@model List<DataAccessLayer.Dtos.SubMenuListDto>

<header id="header" class="fixed-top d-flex align-items-center">
    <div class="container d-flex align-items-center">

        @await Component.InvokeAsync("ContactCompany")
               
        <nav id="navbar" class="navbar order-last order-lg-0">
            <ul>
                @await Component.InvokeAsync("HomeMenu")
              

                 @foreach (var item in Model)
                  {

                <li class="dropdown">
              
                        <a href="@item.Link"><span>@item.HomeMenuName</span> <i class="bi bi-chevron-down"></i></a>
                      

                    <ul>

                            @foreach (var item2 in Model)
                            {
                        <li><a href="@item2.Link">@item2.Name</a></li>

                            }
                    </ul>
                       
                </li>
                }
             
            </ul>
            <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->

     @await Component.InvokeAsync("ContactCompany2")
    </div>
</header><!-- End Header -->><!-- .navbar -->