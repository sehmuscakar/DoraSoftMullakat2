 @model List<DataAccessLayer.Entities.ContactCompany>

 <div class="header-social-links d-flex align-items-center">
      @foreach (var item in Model)
{
            <a href="@item.Twiter" class="twitter"><i class="bi bi-twitter"></i></a>
            <a href="@item.Facebook" class="facebook"><i class="bi bi-facebook"></i></a>
            <a href="@item.Instegram" class="instagram"><i class="bi bi-instagram"></i></a>
            <a href="@item.Linkedin" class="linkedin"><i class="bi bi-linkedin"></i></a>
}
        </div>
